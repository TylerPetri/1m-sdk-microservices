{
  "dashboard": {
    "uid": "sdkms-overview",
    "title": "SDK Microservices - Overview",
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 1,
    "refresh": "10s",
    "tags": [
      "sdk-microservices"
    ],
    "panels": [
      {
        "id": 1,
        "type": "timeseries",
        "title": "Gateway RPS (by route)",
        "datasource": {
          "type": "prometheus",
          "uid": "-100"
        },
        "targets": [
          {
            "expr": "sum by (http_route) (rate(http_server_duration_count[5m]))",
            "legendFormat": "{{http_route}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "req/s"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "type": "timeseries",
        "title": "Gateway p99 latency (s, by route)",
        "datasource": {
          "type": "prometheus",
          "uid": "-100"
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.99, sum by (le, http_route) (rate(http_server_duration_bucket[5m])))",
            "legendFormat": "{{http_route}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
